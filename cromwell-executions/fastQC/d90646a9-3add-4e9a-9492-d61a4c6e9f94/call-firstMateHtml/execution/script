#!/bin/bash

cd /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution
tmpDir=$(mkdir -p "/cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/tmp.72198eef" && echo "/cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/tmp.72198eef")
chmod 777 "$tmpDir"
export _JAVA_OPTIONS=-Djava.io.tmpdir="$tmpDir"
export TMPDIR="$tmpDir"
export HOME="$HOME"
(
cd /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution

)
outd90646a9="${tmpDir}/out.$$" errd90646a9="${tmpDir}/err.$$"
mkfifo "$outd90646a9" "$errd90646a9"
trap 'rm "$outd90646a9" "$errd90646a9"' EXIT
tee '/cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution/stdout' < "$outd90646a9" &
tee '/cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution/stderr' < "$errd90646a9" >&2 &
(
cd /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution


set -euo pipefail
if [[ adrenal_1_fastqc.html != "adrenal_1_fastqc.html" ]];then 
  ln -s /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/inputs/887278789/adrenal_1_fastqc.html "adrenal_1_fastqc.html"
else
  ln -s /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/inputs/887278789/adrenal_1_fastqc.html
fi
)  > "$outd90646a9" 2> "$errd90646a9"
echo $? > /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution/rc.tmp
(
# add a .file in every empty directory to facilitate directory delocalization on the cloud
cd /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution
find . -type d -exec sh -c '[ -z "$(ls -A '"'"'{}'"'"')" ] && touch '"'"'{}'"'"'/.file' \;
)
(
cd /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution
sync


)
mv /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution/rc.tmp /cromwell-executions/fastQC/d90646a9-3add-4e9a-9492-d61a4c6e9f94/call-firstMateHtml/execution/rc
