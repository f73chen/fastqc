#!/bin/bash

cd /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution
tmpDir=$(mkdir -p "/cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/tmp.761ded7f" && echo "/cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/tmp.761ded7f")
chmod 777 "$tmpDir"
export _JAVA_OPTIONS=-Djava.io.tmpdir="$tmpDir"
export TMPDIR="$tmpDir"
export HOME="$HOME"
(
cd /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution

)
out5819c3b9="${tmpDir}/out.$$" err5819c3b9="${tmpDir}/err.$$"
mkfifo "$out5819c3b9" "$err5819c3b9"
trap 'rm "$out5819c3b9" "$err5819c3b9"' EXIT
tee '/cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution/stdout' < "$out5819c3b9" &
tee '/cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution/stderr' < "$err5819c3b9" >&2 &
(
cd /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution


set -euo pipefail
if [[ adrenal_2_fastqc.zip != "adrenal_2_fastqc.zip" ]];then 
  ln -s /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/inputs/1326310089/adrenal_2_fastqc.zip "adrenal_2_fastqc.zip"
else
  ln -s /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/inputs/1326310089/adrenal_2_fastqc.zip
fi
)  > "$out5819c3b9" 2> "$err5819c3b9"
echo $? > /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution/rc.tmp
(
# add a .file in every empty directory to facilitate directory delocalization on the cloud
cd /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution
find . -type d -exec sh -c '[ -z "$(ls -A '"'"'{}'"'"')" ] && touch '"'"'{}'"'"'/.file' \;
)
(
cd /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution
sync


)
mv /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution/rc.tmp /cromwell-executions/fastQC/5819c3b9-7cbc-433d-a66d-2c06e6e2c79d/call-secondMateZip/execution/rc
