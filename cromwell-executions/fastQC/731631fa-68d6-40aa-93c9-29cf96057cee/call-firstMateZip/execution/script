#!/bin/bash

cd /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution
tmpDir=$(mkdir -p "/cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/tmp.0a8c66e5" && echo "/cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/tmp.0a8c66e5")
chmod 777 "$tmpDir"
export _JAVA_OPTIONS=-Djava.io.tmpdir="$tmpDir"
export TMPDIR="$tmpDir"
export HOME="$HOME"
(
cd /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution

)
out731631fa="${tmpDir}/out.$$" err731631fa="${tmpDir}/err.$$"
mkfifo "$out731631fa" "$err731631fa"
trap 'rm "$out731631fa" "$err731631fa"' EXIT
tee '/cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution/stdout' < "$out731631fa" &
tee '/cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution/stderr' < "$err731631fa" >&2 &
(
cd /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution


set -euo pipefail
if [[ adrenal_1_fastqc.zip != "adrenal_1_fastqc.zip" ]];then 
  ln -s /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/inputs/-818823121/adrenal_1_fastqc.zip "adrenal_1_fastqc.zip"
else
  ln -s /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/inputs/-818823121/adrenal_1_fastqc.zip
fi
)  > "$out731631fa" 2> "$err731631fa"
echo $? > /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution/rc.tmp
(
# add a .file in every empty directory to facilitate directory delocalization on the cloud
cd /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution
find . -type d -exec sh -c '[ -z "$(ls -A '"'"'{}'"'"')" ] && touch '"'"'{}'"'"'/.file' \;
)
(
cd /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution
sync


)
mv /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution/rc.tmp /cromwell-executions/fastQC/731631fa-68d6-40aa-93c9-29cf96057cee/call-firstMateZip/execution/rc
