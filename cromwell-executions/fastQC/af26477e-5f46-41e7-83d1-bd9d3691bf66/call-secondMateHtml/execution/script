#!/bin/bash

cd /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution
tmpDir=$(mkdir -p "/cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/tmp.622269e2" && echo "/cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/tmp.622269e2")
chmod 777 "$tmpDir"
export _JAVA_OPTIONS=-Djava.io.tmpdir="$tmpDir"
export TMPDIR="$tmpDir"
export HOME="$HOME"
(
cd /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution

)
outaf26477e="${tmpDir}/out.$$" erraf26477e="${tmpDir}/err.$$"
mkfifo "$outaf26477e" "$erraf26477e"
trap 'rm "$outaf26477e" "$erraf26477e"' EXIT
tee '/cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution/stdout' < "$outaf26477e" &
tee '/cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution/stderr' < "$erraf26477e" >&2 &
(
cd /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution


set -euo pipefail
if [[ adrenal_2_fastqc.html != "adrenal_2_fastqc.html" ]];then 
  ln -s /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/inputs/1204213181/adrenal_2_fastqc.html "adrenal_2_fastqc.html"
else
  ln -s /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/inputs/1204213181/adrenal_2_fastqc.html
fi
)  > "$outaf26477e" 2> "$erraf26477e"
echo $? > /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution/rc.tmp
(
# add a .file in every empty directory to facilitate directory delocalization on the cloud
cd /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution
find . -type d -exec sh -c '[ -z "$(ls -A '"'"'{}'"'"')" ] && touch '"'"'{}'"'"'/.file' \;
)
(
cd /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution
sync


)
mv /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution/rc.tmp /cromwell-executions/fastQC/af26477e-5f46-41e7-83d1-bd9d3691bf66/call-secondMateHtml/execution/rc
